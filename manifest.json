{
  "manifest_version": 2,

  "name": "Youku Tuner",
  "description": "This extension Auto replace player for the current page",
  "version": "1.1",

  "browser_action": {
    "default_icon": "makefg.png",
    "default_title": "Click here to Mannul Excute"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs", 
    "activeTab"
  ]
}
